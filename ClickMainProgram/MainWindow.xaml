<Window x:Class="ClickManagerMain.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ClickManagerMain"
        mc:Ignorable="d"
        Title="Main Page" Height="150" Width="370">
    <Grid>
        <StackPanel Orientation="Vertical">
            <Button Name="ModelManagerBtn" Content="Model Creator" Background="WhiteSmoke" Margin="10,15,10,10" HorizontalAlignment="Center" Width="300" Height="30" Click="ModelManagerBtn_Click" />
            <Button Name="ModelRunnerBtn" Content="Model Runner" Background="WhiteSmoke" Margin="10,15,10,10" HorizontalAlignment="Center" Width="300" Height="30" Click="ModelRunnerBtn_Click" />
            </StackPanel>
    </Grid>
</Window>
